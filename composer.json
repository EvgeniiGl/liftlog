{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2.31",
    "ext-json": "*",
    "ext-sockets": "*",
    "a2way/laravel-tenant-migrate": "^1.0",
    "caouecs/laravel-lang": "~4.0",
    "cboden/ratchet": "^0.4",
    "coderello/laravel-shared-data": "^1.1",
    "darkaonline/l5-swagger": "5.8.*",
    "fideloper/proxy": "^4.0",
    "google/cloud": "^0.130.0",
    "kreait/laravel-firebase": "^2.0",
    "laravel/framework": "5.8.37",
    "laravel/passport": "7.5.1",
    "laravel/tinker": "^1.0",
    "nwidart/laravel-modules": "^4.1",
    "predis/predis": "^1.1",
    "workerman/phpsocket.io": "^1.1",
    "workerman/workerman": "^3.5",
    "zircote/swagger-php": "2.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-laravel5": "^1.0",
    "codeception/module-phpbrowser": "^1.0.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpstan/phpstan": "^0.12.36",
    "phpunit/phpunit": "^7.5",
    "psalm/plugin-laravel": "^1.3",
    "vimeo/psalm": "^3.13"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
